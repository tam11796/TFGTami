<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DFB77B">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ayuda y Preguntas Frecuentes"
        android:textColor="@color/black"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="50dp"
        android:textSize="20dp"
        >

    </TextView>

    <ImageView
        android:id="@+id/imageCamion"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="30dp"
        app:srcCompat="@drawable/camion1" />

    <TextView
        android:id="@+id/textEnvios"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/imageCamion"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="20dp"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:text="Envios" />

    <ImageView
        android:id="@+id/imageDev"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="190dp"
        app:srcCompat="@drawable/ic_baseline_keyboard_return_24" />

    <TextView
        android:id="@+id/textDev"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="185dp"
        android:layout_marginLeft="23dp"
        android:textColor="@color/black"
        android:layout_toRightOf="@id/imageDev"
        android:textSize="20dp"
        android:text="Devoluciones" />


</RelativeLayout>